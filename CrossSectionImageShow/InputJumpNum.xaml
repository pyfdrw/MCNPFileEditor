<Window x:Class="MCNPFileEditor.CrossSectionImageShow.InputJumpNum"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MCNPFileEditor.CrossSectionImageShow"
        mc:Ignorable="d"
        Title="InputJumpNum" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="85" Margin="35,60,0,0" TextWrapping="Wrap" Text="-1" VerticalAlignment="Top" Width="220"/>
        <Button x:Name="button" Content="确定" HorizontalAlignment="Left" Height="40" Margin="75,180,0,0" VerticalAlignment="Top" Width="135" Click="button_Click"/>
        <Label x:Name="label" Content="输入跳转层数，索引从1开始" HorizontalAlignment="Left" Height="25" Margin="35,30,0,0" VerticalAlignment="Top" Width="220"/>
    </Grid>
</Window>
